{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-edtech-etl"
		},
		"ls_adls_edtech_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_edtech'"
		},
		"ls_azure_sql_source_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_azure_sql_source'"
		},
		"ls_adls_edtech_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storagedatalakepunith91.dfs.core.windows.net/"
		},
		"ls_azure_sql_source_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "testsqletl.database.windows.net"
		},
		"ls_azure_sql_source_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "edtechsource"
		},
		"ls_azure_sql_source_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "punith"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-edtech-etl_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_edtech_accountKey": {
						"value": "[parameters('ls_adls_edtech_accountKey')]"
					},
					"ls_azure_sql_source_password": {
						"value": "[parameters('ls_azure_sql_source_password')]"
					},
					"ls_adls_edtech_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_edtech_properties_typeProperties_url')]"
					},
					"ls_azure_sql_source_properties_typeProperties_server": {
						"value": "[parameters('ls_azure_sql_source_properties_typeProperties_server')]"
					},
					"ls_azure_sql_source_properties_typeProperties_database": {
						"value": "[parameters('ls_azure_sql_source_properties_typeProperties_database')]"
					},
					"ls_azure_sql_source_properties_typeProperties_userName": {
						"value": "[parameters('ls_azure_sql_source_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}